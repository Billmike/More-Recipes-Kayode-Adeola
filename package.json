{
  "name": "more-recipes",
  "version": "1.0.0",
  "description": "A recipe sharing web application",
  "main": "app.js",
  "scripts": {
    "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "copy:files": "gulp",
    "server:dev": "nodemon --watch server --watch client --exec babel-node ./server/bin/www",
    "client:dev": "webpack-dev-server --config webpack.config.dev.js && npm run copy:files",
    "start:dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "build:server": "rimraf dist && babel --presets es2015 server -d dist/",
    "build:client": "rimraf build && webpack -d --config webpack.config.dev.js && npm run copy:files && cp -r client/assets/index.html build/index.html",
    "build": "concurrently \"npm run build:server\" \"npm run build:client\"",
    "start": "node dist/bin/www.js",
    "heroku-postbuild": "npm run migrate && npm run build",
    "test": "cross-env NODE_ENV=test && mocha --timeout 10000 --compilers js:babel-core/register server/test/index.test.js",
    "tests": "cross-env NODE_ENV=test nyc _mocha --report lcovonly --reporter spec --timeout 20000 server/test/index.test.js && cat ./coverage/lcov.info | coveralls",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monsieurkayode/More-Recipes.git"
  },
  "keywords": [
    "Javascript",
    "Node",
    "Express",
    "React"
  ],
  "nyc": {
    "require": [
      "babel-register"
    ],
    "exclude": [
      "server/test/"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ],
    "sourceMap": false,
    "instrument": true
  },
  "author": "Kayode Adeola",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/monsieurkayode/More-Recipes/issues"
  },
  "homepage": "https://github.com/monsieurkayode/More-Recipes#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.5.0",
